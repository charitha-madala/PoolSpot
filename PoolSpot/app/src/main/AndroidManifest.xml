<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.misfitcoders">

    <application
        android:allowBackup="true"
        android:icon="@drawable/car"
        android:label="PoolSpot"
        android:roundIcon="@drawable/car"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".NewCard"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".Profile" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:launchMode="singleTop"/>
        <activity android:name=".SignUp_Form" />
        <activity android:name=".Login_Form">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>